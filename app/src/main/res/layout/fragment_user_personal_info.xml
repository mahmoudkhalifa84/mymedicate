<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main_screen.user_profile.health_record.UserMedicalInfo" >

    <Button
        android:id="@+id/b123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:text="@string/ok"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="@string/personal_info"
        android:textColor="@color/main"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintStart_toEndOf="@+id/imageView73"
        app:layout_constraintTop_toTopOf="@+id/imageView73" />

    <ScrollView
        android:id="@+id/scrollView4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView39">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/blod_type"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView

                    android:id="@+id/ed_blod_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView

                    android:id="@+id/ed_blod"
                    android:layout_width="60dp"
                    android:layout_height="25dp"

                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>

            <View

                android:id="@+id/view15"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/height"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/ed_h_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/ed_h"
                    android:layout_width="60dp"
                    android:layout_height="25dp"

                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>
        <View
            
            android:background="@color/gray"
            android:layout_width="wrap_content"
            android:layout_height="1dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/width"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView

                    android:id="@+id/ed_w_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView

                    android:id="@+id/ed_w"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>
        <View
            
            android:background="@color/gray"
            android:layout_width="wrap_content"
            android:layout_height="1dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/sotion_state"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView

                    android:id="@+id/ed_stst_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView

                    android:id="@+id/ed_st"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>
        <View
            
            android:background="@color/gray"
            android:layout_width="wrap_content"
            android:layout_height="1dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/smoking"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView

                    android:id="@+id/ed_smoke_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView

                    android:id="@+id/ed_smo"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>

            <View

                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="16dp"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_weight="2"
                    android:text="@string/drinking"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <TextView

                    android:id="@+id/ed_drink_txt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/known"
                    android:textAlignment="viewStart"
                    android:textSize="16sp" />

                <ImageView

                    android:id="@+id/ed_drok"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:padding="3dp"
                    android:src="@drawable/edit"
                    app:tint="@color/main" />

            </LinearLayout>
        <View
            
            android:background="@color/gray"
            android:layout_width="wrap_content"
            android:layout_height="1dp" />
        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:padding="2dp"
        android:src="@drawable/back_arr_ar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>