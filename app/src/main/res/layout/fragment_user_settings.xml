<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main_screen.settings.Settings">

    <FrameLayout
        android:id="@+id/my_frame_settings"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:id="@+id/ll_hhhide"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@id/my_frame_settings"
            app:layout_constraintEnd_toEndOf="@id/my_frame_settings"
            app:layout_constraintStart_toStartOf="@id/my_frame_settings"
            app:layout_constraintTop_toTopOf="@id/my_frame_settings">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp">

                <LinearLayout
                    android:id="@+id/lll_m"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="8dp"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView25">

                    <LinearLayout
                        android:id="@+id/adv_s_edit_info"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView

                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"
                            android:padding="5dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/settings_user_file"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"

                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/change_user_data"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout

                        android:id="@+id/ll_security"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView

                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"
                            android:padding="5dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/savety"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"

                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/login_and_security"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll_set_lang"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/img_lang"
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"
                            android:padding="5dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/setting_change_language"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/language"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_set_country"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"

                            android:padding="5dp"
                            android:src="@drawable/setting_change_country"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/change_contry"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:text="@string/other"
                    android:textColor="@color/main"
                    android:textSize="24sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lll_m" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:text="@string/action_settings"
                    android:textColor="@color/main"
                    android:textSize="24sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:id="@+id/linearLayoutmmm"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="8dp"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView26">

                    <LinearLayout
                        android:id="@+id/settings_tech_supprot"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/img_shp_row5"
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"

                            android:padding="5dp"
                            android:src="@drawable/settings"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txt_spa_row2"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/tech_support"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:id="@+id/view2"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/settings_report_broblem"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"

                            android:padding="5dp"
                            android:src="@drawable/repr_prp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/rep_prp"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/settings_send_feedback"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"

                            android:padding="5dp"
                            android:src="@drawable/settings_chat"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/send_fed"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_gravity="bottom"
                                android:background="@color/gray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/txt_spa_row23"
                                app:layout_constraintTop_toBottomOf="@+id/img_shp_row23" />
                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/settings_rate_the_app"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="50dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="8dp"

                            android:padding="5dp"
                            android:src="@drawable/rate_us"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:autoText="false"
                                android:gravity="start|center_vertical"
                                android:paddingEnd="16dp"
                                android:text="@string/rate_app"
                                android:textColor="@color/dark_gray"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="@+id/img_shp_row23"
                                app:layout_constraintStart_toEndOf="@+id/img_shp_row23"
                                app:layout_constraintTop_toTopOf="@+id/img_shp_row23" />

                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>